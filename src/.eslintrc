{
  // "extends": ["react-app", "standard"],
  // "rules": {
  //   "space-before-function-paren": 0
  // }
  "parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"worker": true,
		"mocha": true
	},
	"rules": {
		"semi": [
			"error",
			"never"
		],
		"quotes": [
			"error",
			"single",
			{
				"allowTemplateLiterals": true
			}
		],
		"max-len": [
			"error",
			{
				"code": 120
			}
		]
	}
}